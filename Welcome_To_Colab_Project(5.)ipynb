        "import pandas as pd\n",
        "from mlxtend.preprocessing import TransactionEncoder\n",
        "from mlxtend.frequent_patterns import apriori, association_rules\n"
      ],
      "metadata": {
        "id": "aP6qUsrKGgtd"
      },
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "transactions = [\n",
        "    ['Milk', 'Bread', 'Butter'],\n",
        "    ['Bread', 'Butter'],\n",
        "    ['Milk', 'Bread'],\n",
        "    ['Milk', 'Butter'],\n",
        "    ['Bread'],\n",
        "    ['Milk', 'Bread', 'Butter'],\n",
        "    ['Milk', 'Bread'],\n",
        "    ['Butter'],\n",
        "    ['Milk', 'Butter'],\n",
        "    ['Bread', 'Butter']\n",
        "]\n"
      ],
      "metadata": {
        "id": "-EyW2b6nGgw0"
      },
      "execution_count": 2,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "te = TransactionEncoder()\n",
        "te_data = te.fit_transform(transactions)\n",
        "df = pd.DataFrame(te_data, columns=te.columns_)\n"
      ],
      "metadata": {
        "id": "RtlfIhMWGgys"
      },
      "execution_count": 3,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "frequent_itemsets = apriori(df, min_support=0.3, use_colnames=True)"
      ],
      "metadata": {
        "id": "KN_0NMtJG7h-"
      },
      "execution_count": 4,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "rules = association_rules(frequent_itemsets, metric=\"confidence\", min_threshold=0.6)"
      ],
      "metadata": {
        "id": "KXM6oJqdG7Ot"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "rules = rules[['antecedents', 'consequents', 'support', 'confidence', 'lift']]\n",
        "rules = rules.sort_values(by='lift', ascending=False)"
      ],
      "metadata": {
        "id": "GcMg368LGg1V"
      },
      "execution_count": 6,
      "outputs": []
    },
